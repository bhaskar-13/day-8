let areaArray=[];
let circumfernce=[];
class circle
{
    constructor(radius)
    {
        this.radius=radius;
        
    }
 getArea(arr2)
 {
   for(let i=0;i<arr2.length;i++)
   {   let r=arr2[i].radius;
       areaArray.push(3.14*r*r);
   }
   return areaArray;
 }

 getCircumfernce(arr3)
 { 
    for(let i=0;i<arr3.length;i++)
    {   let r=arr3[i].radius;
        circumfernce.push(2*3.14*r);
    }
    return circumfernce;

 }
  
}
let area=new circle();
let area1=new circle(2);
let area2=new circle(3);
let area3=new circle(4);
let arr1=[area1,area2,area3];
console.log(area.getArea(arr1));
console.log(area.getCircumfernce(arr1))

